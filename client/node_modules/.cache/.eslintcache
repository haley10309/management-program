 minWidth: 1080\n  },\n  \n})\nclass App extends Component {\n  // server에서 데이터를 받아오는 방법\n  state = {//component 내에서 변경될 수 있는 변수\n    customers: \"\",\n    completed: 0\n  }\n\n  componentDidMount(){//데이터를 받아오는 작업 , 모든 컴포넌트 준비 완료 후 \n    this.timer = setInterval(this.progress, 20); //0.02초 마다 progress 함수가 재생\n    this.callApi()\n      .then(res => this.setState({customers : res}))\n      .catch(err => console.log(err))\n\n    console.log('얍');\n  }\n\n  callApi = async () => { //비동기적으로 데이터 가져옴\n    const response = await fetch('/api/customers'); //접속하고자 하는 api 주소\n    const body = await response.json(); // 고객 목록을 json 형태로 body 변수에 넣음.\n    return body;\n\n  }\n  progress =() => {\n    const { completed } = this.state;\n    this.setState({ completed : completed >= 100 ? 0 : completed +1 });\n  }\n\n  render(){\n    const { classes } = this.props;\n\n    \n\n\n    return (\n      \n      <Paper className = {classes.root}>\n        \n      <Navbar/>\n     \n        <Table className= {classes.table}>\n       \n          <TableHead>\n            <TableRow> \n            \n              <TableCell>번호</TableCell>\n              <TableCell>이미지</TableCell>\n              <TableCell>이름</TableCell>\n              <TableCell>생년월일</TableCell>\n              <TableCell>성별</TableCell>\n              <TableCell>직업</TableCell>\n              \n            </TableRow>\n          </TableHead>\n          \n          <TableBody>\n          {/* 서버에서 -> state에서 데이터 받으면 -> Customer에 저장 */}\n          {this.state.customers? this.state.customers.map(c =>  { \n            return ( <Customer  key = {c.id } id = {c.id } image = {c.image} name ={c.name } birthday ={c.birthday}  gender ={c.gender} job ={c.job}/>);\n            }): \n            <TableRow>\n              <TableCell colSpan = \"6\" align = \"center\">\n                <CircularProgress className={classes.progress} variant = \"determainate\" value= {this.state.completed}/>\n                </TableCell>\n                </TableRow>\n                }\n        \n          </TableBody>\n          \n        </Table>\n      </Paper>\n      \n    );\n\n\n    \n  }\n\n\n  \n\n}\n\n\nexport default withStyles(styles)(App);\n","/Users/hayungyoo/management_program/client/src/reportWebVitals.js",[],[],"/Users/hayungyoo/management_program/client/src/components/Customer.js",[],[],"/Users/hayungyoo/management_program/client/src/Navbar.js",["60"],[],"\n\n\nexport default function Navbar(){\n\n    const path = window.location.pathname\n\n    return(\n        <nav className=\"nav\">\n            <a href=\"/\" className=\"site-title\">\n                Management program\n            </a>\n            <ul>\n                <li className=\"active\">\n                    <a href=\"/pricing\">Pricing</a>\n                </li>\n                <li>\n                    <a href=\"/about\">About</a>\n                </li>\n            </ul>\n        </nav>\n    )\n}","/Users/hayungyoo/management_program/client/src/pages/about.js",[],[],"/Users/hayungyoo/management_program/client/src/pages/pricing.js",[],[],"/Users/hayungyoo/management_program/client/src/pages/About.js",[],[],"/Users/hayungyoo/management_program/client/src/pages/Pricing.js",[],[],"/Users/hayungyoo/management_program/client/src/pages/Home.js",[],[],{"ruleId":"61","severity":1,"message":"62","line":13,"column":8,"nodeType":"63","messageId":"64","endLine":13,"endColumn":13},{"ruleId":"61","severity":1,"message":"65","line":14,"column":8,"nodeType":"63","messageId":"64","endLine":14,"endColumn":15},{"ruleId":"61","severity":1,"message":"66","line":15,"column":10,"nodeType":"63","messageId":"64","endLine":15,"endColumn":23},{"ruleId":"61","severity":1,"message":"67","line":15,"column":24,"nodeType":"63","messageId":"64","endLine":15,"endColumn":29},{"ruleId":"61","severity":1,"message":"68","line":15,"column":31,"nodeType":"63","messageId":"64","endLine":15,"endColumn":37},{"ruleId":"61","severity":1,"message":"69","line":16,"column":8,"nodeType":"63","me